\";\nimport { getProfile } from \"./services/ApiUserClientService\";\nimport PageNotFound from \"./Components/UI_Aids/PageNotFound\";\nimport SingleTopicPage from \"./Components/SingleTopicPage/SingleTopicPage\";\n\nfunction App() {\n  const [state, dispatch] = Store();\n\n  function getUserProfile(token) {\n    return getProfile(token)\n      .then((res) => res.data)\n      .catch((error) => console.error(error));\n  }\n\n  useEffect(() => {\n    const token = window.localStorage.getItem(\"token\");\n    token &&\n      getUserProfile(token).then((user) => {\n        dispatch({ type: \"user\", payload: user });\n      });\n  }, []);\n\n  return (\n    <>\n      <React.StrictMode>\n        <ThemeProvider theme={customTheme}>\n          <Fonts />\n          <CSSReset />\n          <StateContext.Provider value={{ state, dispatch }}>\n            <NavBar />\n            <Router>\n              <LandingPage path=\"/\" />\n              <AddTopic path=\"/add_topic\" />\n              <Profile path=\"/profile\" />\n              <SingleTopicPage path=\"single_topic/:id\" />\n              <PageNotFound default />\n            </Router>\n          </StateContext.Provider>\n        </ThemeProvider>\n      </React.StrictMode>\n    </>\n  );\n}\n\nexport { StateContext, App };\n","/Users/andreaundecimo/Desktop/Solo Project/client/src/theme/Fonts.js",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/theme/index.js",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/LandingPage/LandingPage.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/NavBar/NavBar.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/theme/colors.js",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/UserForm/UserForm.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/Signup/Signup.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/LoginPage/Login.jsx",[],"/Users/andreaundecimo/Desktop