  });\n      // onClose();\n    } catch (error) {\n      console.error(error);\n    }\n  }\n\n  function handleAddTag(e) {\n    const tagsArray = topicDetails.tags;\n    const { color } = langs.find((lang) => lang.name === e.target.value);\n    const newTag = { color, language: e.target.value };\n    !tagsArray.some((tag) => tag.language === e.target.value) &&\n      tagsArray.push(newTag);\n    setTopicDetails({ ...topicDetails, tags: tagsArray });\n  }\n\n  function handleRemoveTag(e) {\n    const tagsArray = topicDetails.tags;\n    const filteredArray = tagsArray.filter(\n      (el) => el.language !== e.target.value\n    );\n    setTopicDetails({ ...topicDetails, tags: filteredArray });\n  }\n\n  const validateForm = () => {\n    return !topicDetails.title || !topicDetails.content;\n  };\n\n  return (\n    <>\n      {state.isAuth ? (\n        <Flex mt=\"40px\" alignItems=\"center\" py=\"20px\" flexDir=\"column\">\n          <Heading>Submit your answer</Heading>\n          <form onSubmit={handleSubmit} className=\"add-topic-form\">\n            <FormControl\n              isRequired\n              mb=\"20px\"\n              display=\"flex\"\n              flexDir=\"column\"\n              alignItems=\"center\"\n            >\n              <FormLabel>Title</FormLabel>\n              <Input\n                autoFocus=\"false\"\n                w=\"50%\"\n                value={topicDetails.title}\n                type=\"text\"\n                placeholder=\"Title\"\n                onChange={(e) =>\n                  setTopicDetails({ ...topicDetails, title: e.target.value })\n                }\n              ></Input>\n            </FormControl>\n            <FormControl\n              isRequired\n              display=\"flex\"\n              flexDir=\"column\"\n              alignItems=\"center\"\n            >\n              <FormLabel>Content</FormLabel>\n              <Textarea\n                h=\"20vh\"\n                textAlign=\"center\"\n                value={topicDetails.content}\n                type=\"text\"\n                placeholder=\"Write your question here! ðŸ‘¨ðŸ¼â€ðŸ’»\"\n                resize=\"none\"\n                onChange={(e) =>\n                  setTopicDetails({ ...topicDetails, content: e.target.value })\n                }\n              ></Textarea>\n            </FormControl>\n            <Flex justifyContent=\"center\"></Flex>\n            <Flex flexDir=\"column\" justifyContent=\"center\" mt=\"40px\">\n              <Text fontWeight=\"500\" textAlign=\"center\">\n                You can add some categories to your question:\n              </Text>\n              <Box mt=\"20px\">\n                <CheckboxGroup colorScheme=\"green\">\n                  <HStack spacing={5}>\n                    <Checkbox\n                      isChecked={isChecked}\n                      value=\"JavaScript\"\n                      onChange={(e) =>\n                        e.target.checked ? handleAddTag(e) : handleRemoveTag(e)\n                      }\n                    >\n                      JavaScript\n                    </Checkbox>\n                    <Checkbox\n                      value=\"CSS\"\n                      onChange={(e) =>\n                        e.target.checked ? handleAddTag(e) : handleRemoveTag(e)\n                      }\n                    >\n                      CSS\n                    </Checkbox>\n                    <Checkbox\n                      isChecked={isChecked}\n                      value=\"NodeJS\"\n                      onChange={(e) =>\n                        e.target.checked ? handleAddTag(e) : handleRemoveTag(e)\n                      }\n                    >\n                      NodeJS\n                    </Checkbox>\n                    <Checkbox\n                      isChecked={isChecked}\n                      value=\"Python\"\n                      onChange={(e) =>\n                        e.target.checked ? handleAddTag(e) : handleRemoveTag(e)\n                      }\n                    >\n                      Python\n                    </Checkbox>\n                    <Checkbox\n                      isChecked={isChecked}\n                      value=\"TypeScript\"\n                      onChange={(e) =>\n                        e.target.checked ? handleAddTag(e) : handleRemoveTag(e)\n                      }\n                    >\n                      TypeScript\n                    </Checkbox>\n                  </HStack>\n                </CheckboxGroup>\n              </Box>\n            </Flex>\n            <Button\n              disabled={validateForm()}\n              mt={10}\n              type=\"submit\"\n              colorScheme=\"teal\"\n              variant=\"outline\"\n              boxShadow=\"sm\"\n              _hover={{ boxShadow: \"md\" }}\n              _active={{ boxShadow: \"lg\" }}\n            >\n              Submit\n            </Button>\n            <ScaleFade initialScale={0.6} in={isOpen}>\n              <Box\n                p=\"40px\"\n                color=\"white\"\n                mt=\"4\"\n                bg=\"teal.500\"\n                rounded=\"md\"\n                shadow=\"md\"\n              >\n                Your post has been created! âœ…\n              </Box>\n            </ScaleFade>\n          </form>\n        </Flex>\n      ) : (\n        <Heading>Not auth</Heading>\n      )}\n    </>\n  );\n}\n","/Users/andreaundecimo/Desktop/Solo Project/client/src/global.context/globalStore.reducer.js",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/Subtitle.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/Profile/Profile.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/TopicCard/TopicCard.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/TopicCard/TopicCardProfile.jsx",[],"/Users/andreaundecimo/Desktop/Solo Project/client/src/Components/UI_Aids/ErrorMessage/ErrorMessage.